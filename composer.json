{
    "type": "symfony-bundle",
    "name": "misiyuk/math-bundle",
    "description": "Math bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Misiyuk"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Misiyuk\\MathBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Misiyuk\\MathBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
